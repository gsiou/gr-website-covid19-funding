{

  "manifest_version": 2,
  "name": "Media List",
  "description": ".",
  "version": "1.0",
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "54.0a1"
    }
  },

  "browser_action": {
    "default_icon": "icons/icon.svg",
    "default_title" : "List",
    "default_popup": "popup/popup.html"
  },

  "background": {
    "scripts": ["data.js", "background.js"]
  },

  "permissions": ["tabs"]
}
